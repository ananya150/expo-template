    "expo-linking": "~7.0.3",
    "expo-router": "~4.0.15",
    "expo-status-bar": "~2.0.0",
    "expo-system-ui": "~4.0.6",
    "expo-updates": "~0.26.10",
    "react": "18.3.1",
    "react-native": "0.76.5",
    "react-native-edge-to-edge": "^1.1.3",
    "react-native-gesture-handler": "~2.20.2",
